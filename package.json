{
  "name": "express-prisma",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "dev": "dotenv -e .env.dev -- ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "preprod": "dotenv -e .env -- prisma migrate deploy",
    "prod": "dotenv -e .env -- pm2 start --name api-rest-prisma -i 2 dist/server.js",
    "postprod": "pm2 logs",
    "start": "npm-run-all build prod",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.27.6",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "prisma": "^3.1.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@prisma/client": "^3.1.1",
    "bcrypt": "^5.0.1",
    "celebrate": "^15.0.0",
    "dotenv-cli": "^4.0.0",
    "express": "^4.17.1",
    "ioredis": "^4.27.10",
    "jsonwebtoken": "^8.5.1",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.1.2"
  }
}
